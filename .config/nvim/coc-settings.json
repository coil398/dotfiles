{
  "suggest.noselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "args": ["--lsp"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["vim", "markdown", "yaml"]
    },
    "terraform-ls": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "terraform-lsp": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "coc.preferences.formatOnSaveFiletypes": ["rust", "c", "cpp", "go", "python", "ts", "typescriptreact", "css", "markdown"],
  "coc.preferences.useQuickfixForLocations": true,
  "eslint.autoFixOnSave": true,
  "explorer.icon.enableNerdfont": false,
  "rust-analyzer.updates.channel": "nightly",
  "python.formatting.provider": "black",
  "deno.lint": true,
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
}
